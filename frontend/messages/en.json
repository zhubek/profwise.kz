      "title": "Holland Test (RIASEC)",
      "subtitle": "Discover Your Career Interests",
      "overview": "The Holland Test, also known as the RIASEC Interest Inventory, is a comprehensive career assessment tool developed by psychologist John L. Holland. This scientifically validated test helps you understand your vocational interests and identifies career paths that align with your natural preferences and personality.",
      "whatItMeasures": {
        "title": "What This Test Measures",
        "description": "The Holland Test evaluates your interests across six personality types, known as the RIASEC model:",
        "realistic": "Realistic (R): Practical, hands-on problem solvers who enjoy working with tools, machines, and physical tasks.",
        "investigative": "Investigative (I): Analytical thinkers who love research, inquiry, and solving complex problems.",
        "artistic": "Artistic (A): Creative individuals who express themselves through art, music, writing, or design.",
        "social": "Social (S): People-oriented helpers who enjoy teaching, counseling, and supporting others.",
        "enterprising": "Enterprising (E): Leaders and persuaders who thrive in business, sales, and entrepreneurial roles.",
        "conventional": "Conventional (C): Organized detail-oriented individuals who excel at data management and structured tasks."
      },
      "howItWorks": {
        "title": "How It Works",
        "step1": "Answer 60 questions honestly about activities you enjoy or would like to do.",
        "step2": "Rate your interest level in various work-related tasks and scenarios.",
        "step3": "Receive your personalized RIASEC profile showing your top interest areas.",
        "step4": "Explore career matches tailored to your unique interest profile."
      },
      "instructions": {
        "title": "Instructions",
        "intro": "To get the most accurate results:",
        "point1": "Set aside 15-20 minutes in a quiet environment.",
        "point2": "Answer honestly based on what you genuinely enjoy, not what others expect.",
        "point3": "Don't overthink your answers—your first instinct is usually correct.",
        "point4": "Complete all sections for comprehensive results.",
        "point5": "You can save your progress and return later if needed."
      },
      "resultsInfo": {
        "title": "What You'll Learn",
        "point1": "Your top three Holland Code types (e.g., IAS, RIE)",
        "point2": "Detailed breakdown of your interest scores across all six categories",
        "point3": "Personalized career recommendations matched to your profile",
        "point4": "Insights into work environments where you'll thrive",
        "point5": "Understanding of your natural strengths and preferences"
      }
    }
